	//한글
	function setBg(a){
		if(!a.value) {
			a.className="fill"
		}
	}

	function cleBg(a){
		if(a.value == '') {
			a.className="empty"
		}
	}

	//영문
	function setBgE(a){
		if(!a.value) {
			a.className="fillE"
		}
	}

	function cleBgE(a){
		if(a.value == '') {
			a.className="emptyE"
		}
	}
	</SCRIPT>      
