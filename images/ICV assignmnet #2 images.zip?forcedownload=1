<script type="text/javascript">

var ExpDate = new Date();

function multiSubmit() {
	var f = document.login_form;
	//2008.4.1. 배현아, ID 끝자리가 *인 경우 제어 시작
	var str_temp   = f.si_id.value;
	var str_len    = str_temp.length;
	var str_charat = str_temp.charAt(str_len-1);
	//2008.4.1. 배현아, ID 끝자리가 *인 경우 제어 끝
	if(f.si_id.value==""){
			alert('아이디를 입력하세요');
		//	clearOverID(); 2012.5.9. 윤미연, 보건진료소 요청으로 주석
			f.si_id.focus();
			return false;
	}
	if(f.si_pwd.value==""){
			alert('비밀번호를 입력하세요');
//			clearOverPW();   2012.5.9. 윤미연, 보건진료소 요청으로 주석
			f.si_pwd.focus();
			return false;
	}
	//2008.4.1. 배현아, ID 끝자리가 *인 경우 제어 시작
	if(str_charat=="*"){
			alert('아이디를 정확하게 입력하세요.');
	//		clearOverID();     2012.5.9. 윤미연, 보건진료소 요청으로 주석
	//		clearOverPW();     2012.5.9. 윤미연, 보건진료소 요청으로 주석
			f.si_id.focus();
			return false;
	}       
	//2008.4.1. 배현아, ID 끝자리가 *인 경우 제어 끝
	if(f.id_save.checked==true){
			SetCookieVal("use_id", "t",null,null,"snu.ac.kr");
			SetCookieVal("saved_id", f.si_id.value,null,null,"snu.ac.kr");
	} else {
			deleteCookie("use_id","/","snu.ac.kr");
			deleteCookie("saved_id","/","snu.ac.kr");
	}

	f.action="https://sso.snu.ac.kr/3rdParty/idPasswordLogin.jsp?UURL=https%3A%2F%2Fsso.snu.ac.kr%2Fnls3%2Ffcs&NONCE=JdNAFlYc1eYRejCHVLhGSRok8QG4HBgjypC9mSDwW3Ayty8c%2FGNgEPueWJkLgGj9e7zZQtylTaT8mATmMR1b6Q%3D%3D&InitechEamNoCacheNonce=hYBBVr84ncOJUBaa2VY39Q%3D%3D";
	f.submit();
}

function getCookieVal (offset) {
   var endstr = document.cookie.indexOf (";", offset);
   if (endstr == -1) endstr = document.cookie.length;
   return unescape(document.cookie.substring(offset, endstr));
}

function GetCookie (name) {
   var arg = name + "=";
   var alen = arg.length;
   var clen = document.cookie.length;
   var i = 0;
   while (i < clen) { //while open
          var j = i + alen;
          if (document.cookie.substring(i, j) == arg)
                 return getCookieVal (j);
          i = document.cookie.indexOf(" ", i) + 1;
          if (i == 0) break;
   } //while close
        return null;
}

function SetCookie (name, value) {
   var argv = SetCookie.arguments;
   var argc = SetCookie.arguments.length;
   var expires = (2 < argc) ? argv[2] : null;
   var path = (3 < argc) ? argv[3] : null;
   var domain = (4 < argc) ? argv[4] : null;
   var secure = (5 < argc) ? argv[5] : false;
   document.cookie = name + "=" + escape (value) +
          ((expires == null) ? "" : ("; expires=" + expires.toGMTString())) +
          ((path == null) ? "" : ("; path=" + path)) +
          ((domain == null) ? "" : ("; domain=" + domain)) +
          ((secure == true) ? "; secure" : "");
}

function SetCookieVal (key,value) {
   pathname = location.pathname;
   var myDomain = pathname.substring(0, pathname.lastIndexOf('/')) +'/';
   ExpDate.setTime(ExpDate.getTime() + 1000*60*60*24*30);
   SetCookie(key, value, ExpDate, myDomain);
}


function deleteCookie (name,path,domain) {
  var expires = new Date(1970, 1, 1, 12, 1, 1);
  if (GetCookie(name)) {
        document.cookie = name + "=" + ((path) ? "; path=" + path : "") + ((domain) ? "; domain=" + domain : "") + "; expires=" + expires.toGMTString();
  }
}

function clearOverID(){
        document.getElementById("overID").style.display="none";
        document.getElementById("si_id_input").style.display="block";
        document.login_form.si_id.focus();
}
function clearOverPW(){
        document.getElementById("overPW").style.display="none";
        document.getElementById("si_pwd_input").style.display="block";
        document.login_form.si_pwd.focus();
}

//2014.02.10 신청화면 관련 팝업창으로 전환
function next_page1(file_name, is_pop) {
	var wWidth = 821;  
	var wHight = 600;

	var wX = (window.screen.width - wWidth) / 2;
	var wY = (window.screen.height - wHight - 100) / 2;  

	window.open("https://idm.snu.ac.kr/"+file_name+"?is_pop="+is_pop,"", "left="+wX+",top="+wY+",width="+wWidth+",height="+wHight+",scrollbars=no,status=no,resizable=yes,menubar=no");
}
</script>
<script type="text/javascript">
        var use_id = GetCookie("use_id");
        var saved_id = GetCookie("saved_id");

        if(use_id == "t") {
                document.login_form.si_id.value=saved_id;
                document.login_form.id_save.checked=true;
        } else {
                document.login_form.id_save.checked=false;
        }

        document.login_form.id_save.focus=focus();
</script>


